{% extends 'site/media/page.html.twig' %}

{% block pageTitle %}History of changes of {% if media.title %}{{ media.title }}{% else %}Media{% endif %} - {% endblock %}



{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
	{{ parent() }}
{% endblock %}

{% block tabHistory %}currentTab{% endblock %}

{% block innerContent %}

	<ul class="history">
		{% for historyItem in historyItems %}
			{%  if historyItem.siteWebTemplate %}
				{%  include historyItem.siteWebTemplate with { 'historyItem':historyItem } %}
			{% endif %}
		{% endfor %}
	</ul>

{% endblock %}


