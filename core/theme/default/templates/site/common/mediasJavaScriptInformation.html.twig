<script type="text/javascript">
	var mediaData = [
		{% for media in medias %}
			{
				'slug':{{  media.slug }},
				'title': "{{ media.title |e('js') }}",
				'sourceText': "{{ media.sourceText |e('js') }}",
				'sourceURL': "{{ media.sourceURL |e('js') }}",
			} {% if not loop.last %},{% endif %}
		{% endfor %}	
	];
</script>
