{% extends 'site/event/page.html.twig' %}




{% block pageTitle %}Cancel {{ event.summaryDisplay }} - {% endblock %}


{% block htmlHeader %}
	<META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW">
	{{ parent() }}
{% endblock %}


{% block innerContent %}

	
	<form action="#" method="post" class="styled">

		{{ form_errors(form) }}

		{{ form_row(form.agree) }}

		{%  include 'site/common/formRowEditComment.html.twig' with {'form': form} %}

		{{ form_rest(form) }}

		<div class="bigSubmitActionWrapper"><input type="submit"  value="Cancel" class="bigSubmitAction"/></div>
		<div class="afterBigSubmitActionWrapper"></div>
	</form>



{% endblock %}

