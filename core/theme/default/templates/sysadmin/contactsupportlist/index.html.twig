{% extends 'sysadmin/page.html.twig' %}

{% block pageTitle %}Contact Support - {% endblock %}

{% block content %}

	<table class="data">
		<tr>
			<th>ID</th>
			<th>Subject</th>
			<th>Start</th>
			<th>&nbsp;</th>
		</tr>
		{% for contactsupport in contactsupports %}
			<tr {% if loop.index is even %}class="alternative"{% endif %} >
				<td>{{ contactsupport.id }}</td>
				<td>{{ contactsupport.subject }}</td>
				<td>
					<a href="/sysadmin/contactsupport/{{ contactsupport.id }}" class="button">Show</a>
				</td>
			</tr>
		{% endfor %}
	</table>

{% endblock %}

